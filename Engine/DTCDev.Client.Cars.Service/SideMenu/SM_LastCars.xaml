<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:SideMenuHelp="clr-namespace:DTCDev.Client.Cars.Service.SideMenu.SideMenuHelp" x:Class="DTCDev.Client.Cars.Service.SideMenu.SM_LastCars" 
             mc:Ignorable="d" 
             d:DesignHeight="300" Width="240" Loaded="UserControl_Loaded">
    <Grid>
        <Border BorderBrush="#FFD0D1D7" BorderThickness="2,0,2,0"/>
        <StackPanel x:Name="wrp" Margin="20,60,20,20"/>
        <SideMenuHelp:HeaderLine x:Name="headerLine" VerticalAlignment="Top" CloseClick="headerLine_CloseClick" />
    </Grid>
</UserControl>
