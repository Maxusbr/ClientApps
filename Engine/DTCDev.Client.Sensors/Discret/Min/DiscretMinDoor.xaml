<UserControl x:Class="DTCDev.Client.Sensors.Discret.Min.DiscretMinDoor"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d" Width="31" Height="31" x:Name="control" Loaded="UserControl_Loaded">
	
	<Grid x:Name="LayoutRoot">
        <Image Source="/DTCDev.Client.Sensors;component/Assets/Door.png" Stretch="None"  VerticalAlignment="Top" HorizontalAlignment="Left" Margin="5"/>
        <Image x:Name="imgAlarm" Source="/DTCDev.Client.Sensors;component/Assets/Door_alarm.png" Stretch="None" Visibility="{Binding ElementName=control, Path=DisPresenter.VisAlarm}" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="5"/>
	</Grid>
</UserControl>
