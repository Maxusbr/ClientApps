<UserControl x:Class="DTCDev.Client.Sensors.Analog.Min.BigShock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="32" d:DesignWidth="32">
    <Grid>
        <Path Data="M 5,5 l 4,20 l 4,-20 l 4,20 l 4,-20 l 4,20" Stroke="Black" StrokeThickness="1">
        </Path>
        <Path  Visibility="{Binding Path=AlarmVisibility}" Data="M 5,5 l 4,20 l 4,-20 l 4,20 l 4,-20 l 4,20" Stroke="Green" StrokeThickness="3">
        </Path>
    </Grid>
</UserControl>
