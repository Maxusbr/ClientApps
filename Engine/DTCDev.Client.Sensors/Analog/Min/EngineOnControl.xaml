<UserControl x:Class="DTCDev.Client.Sensors.Analog.Min.EngineOnControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Height="25" Width="25">
    
    <Grid x:Name="LayoutRoot">
        <Ellipse x:Name="elpOk" Visibility="Collapsed" Fill="LightGreen" Margin="3">
            <Ellipse.Effect>
                <BlurEffect Radius="5"/>
            </Ellipse.Effect>
        </Ellipse>
        <Image Width="24" Height="24" VerticalAlignment="Top" HorizontalAlignment="Left" Source="/DTCDev.Client.Sensors;component/Assets/Images/Engine.png" />
    </Grid>
</UserControl>
